{
  "name": "@blog/pwa",
  "version": "6.26.36",
  "description": "progressive web application tools including workbox + precaching",
  "keywords": [
    "pwa",
    "workbox",
    "prerender"
  ],
  "author": "aquariuslt <superaquariuslt@gmail.com>",
  "homepage": "https://github.com/aquariuslt/blog#readme",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aquariuslt/blog.git"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build:lib": "tsc",
    "patch": "patch-package",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@blog/common": "^6.26.36",
    "lodash": "4.17.15",
    "patch-package": "6.2.2",
    "postinstall-postinstall": "2.1.0",
    "workbox-build": "4.3.1"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/node": "13.13.5",
    "jest": "26.0.1",
    "jest-raw-loader": "1.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.5.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.*\\.md$": "jest-raw-loader"
    },
    "collectCoverageFrom": [
      "!**/__tests__/**",
      "<rootDir>/src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "<rootDir>/coverage"
  }
}
