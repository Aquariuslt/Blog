{
  "name": "@blog/pwa",
  "version": "6.26.213",
  "description": "progressive web application tools including workbox + precaching",
  "keywords": [
    "pwa",
    "workbox",
    "prerender"
  ],
  "author": "aquariuslt <superaquariuslt@gmail.com>",
  "homepage": "https://github.com/aquariuslt/blog#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aquariuslt/blog.git"
  },
  "private": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "build:lib": "tsc",
    "patch": "patch-package",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@blog/common": "^6.26.213",
    "lodash": "4.17.21",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "workbox-build": "6.1.5"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/lodash": "4.14.168",
    "@types/node": "13.13.48",
    "jest": "26.6.3",
    "jest-raw-loader": "1.0.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.*\\.md$": "jest-raw-loader"
    },
    "collectCoverageFrom": [
      "!**/__tests__/**",
      "<rootDir>/src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "<rootDir>/coverage"
  }
}
