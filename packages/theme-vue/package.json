{
  "name": "@blog/theme-vue",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "compile:config": "tsc vue.config.ts",
    "debug:webpack": "vue inspect > webpack.config.js",
    "postinstall": "tsc vue.config.ts",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@blog/common": "^6.0.0",
    "@blog/routes-tools": "^6.0.0",
    "@mdi/font": "^4.5.95",
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.3.0",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.1",
    "@vue/cli-plugin-pwa": "^4.0.4",
    "@vue/cli-plugin-typescript": "^4.0.4",
    "@vue/cli-plugin-unit-jest": "^4.0.4",
    "@vue/cli-service": "^4.0.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "cosmiconfig": "^5.2.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@theme/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
