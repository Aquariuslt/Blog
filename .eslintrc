{
  "root": true,
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "html",
    "babel",
    "react"
  ],
  "rules": {
    "semi": [
      "error",
      "always",
      {
        "omitLastInOneLineBlock": true
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "require-jsdoc": "off",
    "prefer-rest-params": "off",
    "space-before-function-paren": "off",
    "max-len": "off",
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "eol-last": "off",
    "no-invalid-this": "off"
  }
}
