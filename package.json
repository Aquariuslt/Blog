    "sw-precache": "^5.2.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.21.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.3",
    "webpack-pwa-manifest": "^3.7.1",
    "xml": "^1.0.1",
    "yorkie": "^2.0.0"
  },
  "workspaces": [
    "packages/blog-markdown-util"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "/config/**",
    "/tasks/**",
    "/coverage/**",
    "/packages/**"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "setupFiles": [
      "<rootDir>/test/unit/shared/hooks/setup"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!**/config/**",
      "!**/tasks/**",
      "!**/packages/**",
      "!**/node_modules/**"
    ]
  }
}
