    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.7.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-inject-string": "^1.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-rimraf": "^0.2.2",
    "gulp-sequence": "^1.0.0",
    "gulp-yaml": "^2.0.1",
