    "@types/moxios": "^0.4.8",
    "@types/sinon": "^5.0.5",
    "@types/webpack": "^4.4.17",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/test-utils": "1.0.0-beta.25",
    "ajv": "^6.5.4",
    "autoprefixer": "^9.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
